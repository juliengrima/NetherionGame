{% extends 'base.html.twig' %}

{% block title %}ZonesText index{% endblock %}

{% block body %}
    <h1>ZonesText index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Texts</th>
                <th>Title</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for zones_text in zones_texts %}
            <tr>
                <td>{{ zones_text.id }}</td>
                <td>{{ zones_text.texts }}</td>
                <td>{{ zones_text.title }}</td>
                <td>
                    <a href="{{ path('app_zones_text_show', {'id': zones_text.id}) }}">show</a>
                    <a href="{{ path('app_zones_text_edit', {'id': zones_text.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_zones_text_new') }}">Create new</a>
{% endblock %}
