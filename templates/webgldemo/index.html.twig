    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Lin</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for web_g_l_demo in web_g_l_demos %}
